				</bean>
				<bean class="com.dv.exzcercise.Employee" name="emp2">
					<property name="id" value="2" />
					<property name="name" value="Thiyagu" />
					<property name="sal" value="20000000" />


				</bean>

			</list>
		</property>
	</bean>

</beans>